                .progresed
                  .progresed-indicator 60%
                  .progresed-bar
                    .progresed-bar-complete(style='width:64%')
            .col-md-3.col-sm-4
              .efecto
                .bg-circle
                  img.img-responsive(src='img/bootstrap.png')
                h5.pt-5(style='margin-top: 4px;') Bootstrap/Responsive
                .progresed
                  .progresed-indicator 75%
                  .progresed-bar
                    .progresed-bar-complete(style='width:80%')
            .col-md-3.col-sm-4
              .efecto
                i.ion-social-wordpress
                h5.pt-5 Wordpress
                .progresed
                  .progresed-indicator 50%
                  .progresed-bar
                    .progresed-bar-complete(style='width:50%')
